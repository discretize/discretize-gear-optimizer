{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "ignorePatterns": [
        "gulpfile.js"
    ],
    "extends": "semistandard",
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1,
                "MemberExpression": 1
            }
        ],
        "quote-props": [
            "warn",
            "consistent"
        ],
        "padded-blocks": "off",
        "operator-linebreak": [
            "warn",
            "before"
        ],
        "no-throw-literal": "off",
        "dot-notation": "off",
        "no-prototype-builtins": "off",
        "wrap-iife": "off",
        "no-unused-vars": "warn"
    }
}