{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "semi": 1,
        "no-prototype-builtins": "off",
        "linebreak-style": "off",
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1,
                "MemberExpression": 1
            }
        ],
        "operator-linebreak": [
            "warn",
            "before"
        ],
        "comma-spacing": "off",
        "no-shadow": "off",
        "space-before-function-paren": "off",
        "id-length": "off",
        "dot-notation": "off",
        "no-unused-vars": [
            "error",
            {
                "vars": "local"
            }
        ],
        "no-multiple-empty-lines": [
            "warn",
            {
                "max": 1
            }
        ]
    }
}