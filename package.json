{
  "name": "discretize-gear-optimizer",
  "version": "1.0.0",
  "private": true,
  "description": "Discretize Gear Optimizer",
  "author": "marcustyphoon, gw2princeps, metagame",
  "packageManager": "pnpm@8.15.8+sha512.d1a029e1a447ad90bc96cd58b0fad486d2993d531856396f7babf2d83eb1823bb83c5a3d0fc18f675b2d10321d49eb161fece36fe8134aa5823ecd215feed392",
  "engines": {
    "node": ">=16",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "type": "module",
  "browserslist": [
    "defaults and supports es6-module"
  ],
  "scripts": {
    "develop": "vite",
    "dev": "vite",
    "start": "vite",
    "dev-cf": "VITE_HAS_CF=true wrangler pages dev --kv SHORT_LINKS -- pnpm dev",
    "build": "vite build",
    "serve": "vite preview --host",
    "build-cf": "VITE_HAS_CF=true vite build",
    "serve-cf": "VITE_HAS_CF=true wrangler pages dev --kv SHORT_LINKS -- pnpm serve",
    "build-gh": "vite build --base /discretize-gear-optimizer/",
    "build-all": "concurrently -g -c auto \"pnpm:build-cf --outDir dist-cf\" \"pnpm:build-gh --outDir dist-gh\"",
    "prettier": "prettier --write . \"!**/*.{md,json,yaml,yml}\"",
    "prettierCheck": "prettier --check . \"!**/*.{md,json,yaml,yml}\"",
    "eslint": "eslint --ignore-path .gitignore --max-warnings 0 . --ext .js,.jsx,.ts,.tsx",
    "typeCheck": "tsc --noEmit",
    "checkAll": "concurrently -g -c auto pnpm:testData pnpm:validateDataTypes pnpm:eslint pnpm:prettierCheck pnpm:typeCheck pnpm:wasmCheck",
    "updateMappings": "node updateMappings.js",
    "testData": "tsx ./src/assets/testData.js",
    "validateDataTypes": "typia generate --input ./src/assets/typevalidation/ --output ./src/assets/typevalidation/output && tsx ./src/assets/typevalidation/output",
    "extractLocale": "babel --config-file ./babel-extract.config.js 'src/**/*.{js,jsx,ts,tsx}' > /dev/null",
    "extractLocaleEn": "babel --config-file ./babel-extract-en.config.js 'src/**/*.{js,jsx,ts,tsx}' > /dev/null && node cleanupLocale.js",
    "extractLocaleCMD": "babel --config-file ./babel-extract.config.js \"src/**/*.{js,jsx,ts,tsx}\" > NUL",
    "cleanupLocale": "node cleanupLocale.js",
    "wasm": "wasm-pack build ./wasm_module --target web --release",
    "wasm-build-data": "tsx generateRustMappings.ts",
    "wasmCheck": "cd wasm_module && cargo check --all-targets"
  },
  "resolutions": {
    "eslint-plugin-jsx-a11y": "^6.5.1 <6.8.0"
  },
  "dependencies": {
    "@discretize/globals": "^0.0.2",
    "@discretize/gw2-ui-new": "^0.1.5",
    "@discretize/object-compression": "^1.0.3",
    "@discretize/react-discretize-components": "^2.6.1",
    "@discretize/typeface-menomonia": "^0.1.3",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.18",
    "@mui/material": "^5.15.18",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.2",
    "classnames": "^2.3.1",
    "comlink": "^4.4.1",
    "i18next": "^23.11.5",
    "js-yaml": "^4.1.0",
    "json-url": "^3.0.0",
    "material-ui-popup-state": "^5.1.2",
    "pako": "^2.0.4",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-fast-compare": "^3.2.2",
    "react-i18next": "^14.1.2",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-saga": "^1.1.3",
    "tss-react": "^4.9.10",
    "typed-redux-saga": "^1.5.0",
    "typeface-fira-mono": "^1.1.13",
    "typeface-muli": "^1.1.13",
    "typeface-raleway": "^1.1.13",
    "uint8array-extras": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.24.6",
    "@babel/plugin-transform-modules-commonjs": "^7.24.6",
    "@babel/plugin-transform-typescript": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@babel/types": "^7.24.6",
    "@cloudflare/workers-types": "^4.20240512.0",
    "@jambit/eslint-plugin-typed-redux-saga": "^0.4.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/babel__core": "^7.1.19",
    "@types/js-yaml": "^4.0.6",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-react": "^4.3.0",
    "babel-plugin-i18next-extract": "^0.10.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^48.2.6",
    "eslint-plugin-jsx-a11y": "^6.5.1 <6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.2.5",
    "rollup-preserve-directives": "^1.1.1",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "typia": "^6.0.4",
    "vite": "^5.2.11",
    "vite-plugin-comlink": "^5.0.1",
    "vite-plugin-wasm": "^3.3.0",
    "wasm-pack": "^0.12.1",
    "wrangler": "^3.57.1"
  }
}
