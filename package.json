{
  "name": "discretize-gear-optimizer",
  "version": "1.0.0",
  "private": true,
  "description": "Discretize Gear Optimizer",
  "author": "marcustyphoon, gw2princeps, metagame",
  "engines": {
    "node": ">=16",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "browserslist": [
    "defaults and supports es6-module"
  ],
  "scripts": {
    "develop": "vite",
    "dev": "vite",
    "start": "vite",
    "cfdev": "VITE_HAS_CF=true wrangler pages dev --kv SHORT_LINKS -- pnpm dev",
    "build": "vite build",
    "cfbuild": "VITE_HAS_CF=true vite build --watch",
    "serve": "vite preview --host",
    "cfserve": "VITE_HAS_CF=true wrangler pages dev --kv SHORT_LINKS -- pnpm serve",
    "buildserve": "vite build && vite preview --host",
    "prettier": "prettier --write . \"!**/*.{md,json,yaml,yml}\"",
    "prettierCheck": "prettier --check . \"!**/*.{md,json,yaml,yml}\"",
    "eslint": "eslint --ignore-path .gitignore --max-warnings 0 . --ext .js,.jsx,.ts,.tsx",
    "updateMappings": "node updateMappings.js",
    "testData": "node ./src/assets/testData.js",
    "testModifiers": "echo 'this script has been replaced with testData!'",
    "testPresets": "echo 'this script has been replaced with testData!'",
    "extractLocale": "babel --config-file ./babel-extract.config.js 'src/**/*.{js,jsx,ts,tsx}' > /dev/null",
    "extractLocaleEn": "babel --config-file ./babel-extract-en.config.js 'src/**/*.{js,jsx,ts,tsx}' > /dev/null && node cleanupLocale.js",
    "extractLocaleCMD": "babel --config-file ./babel-extract.config.js \"src/**/*.{js,jsx,ts,tsx}\" > NUL",
    "cleanupLocale": "node cleanupLocale.js",
    "wasm": "wasm-pack build ./wasm_module --target web --release",
    "wasm-build-data": "tsx generateRustMappings.ts",
    "wasmCheck": "cd wasm_module && cargo check",
    "wasmCheckAll": "cd wasm_module && cargo check --all-targets"
  },
  "resolutions": {
    "@discretize/gw2-ui-new": "^0.1.2",
    "eslint-plugin-jsx-a11y": "^6.5.1 <6.8.0"
  },
  "dependencies": {
    "@discretize/globals": "^0.0.2",
    "@discretize/gw2-ui-new": "^0.1.4",
    "@discretize/object-compression": "^1.0.3",
    "@discretize/react-discretize-components": "^2.6.1",
    "@discretize/typeface-menomonia": "^0.1.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.15.9",
    "@mui/material": "^5.15.9",
    "@reduxjs/toolkit": "^2.0.1",
    "@vitejs/plugin-react": "^4.0.0",
    "arraybuffer-equal": "^1.0.4",
    "axios": "^1.5.1",
    "buffer": "^6.0.3",
    "classnames": "^2.3.1",
    "i18next": "^23.8.2",
    "js-yaml": "^4.1.0",
    "json-url": "^3.0.0",
    "material-ui-popup-state": "^5.0.8",
    "pako": "^2.0.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-i18next": "^14.0.5",
    "react-redux": "^9.1.0",
    "redux": "^5.0.1",
    "redux-saga": "^1.1.3",
    "tss-react": "^4.9.4",
    "typed-redux-saga": "^1.5.0",
    "typeface-fira-mono": "^1.1.13",
    "typeface-muli": "^1.1.13",
    "typeface-raleway": "^1.1.13",
    "urlsafe-base64": "^1.0.0",
    "vite": "^5.1.1",
    "vite-plugin-wasm": "^3.3.0",
    "wasm-pack": "^0.12.1",
    "wrangler": "^3.28.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/plugin-transform-typescript": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/types": "^7.16.7",
    "@cloudflare/workers-types": "^4.20240208.0",
    "@jambit/eslint-plugin-typed-redux-saga": "^0.4.0",
    "@types/babel__core": "^7.1.19",
    "@types/js-yaml": "^4.0.6",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-plugin-i18next-extract": "^0.10.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^48.0.6",
    "eslint-plugin-jsx-a11y": "^6.5.1 <6.8.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2"
  }
}
