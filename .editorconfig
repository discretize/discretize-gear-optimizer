# editorconfig.org
root = true

[*]
end_of_line = lf

[*.html]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true

[*.js]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true

[*.yaml]
trim_trailing_whitespace = true
indent_style = space
indent_size = 2